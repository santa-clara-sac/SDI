<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule"
           scriptProcessor="C:\Users\Admin\Desktop\SDI\.venv\Scripts\python.exe|C:\Users\Admin\Desktop\SDI\.venv\Lib\site-packages\wfastcgi.py"
           resourceType="Unspecified" requireAccess="Script" />
    </handlers>

    <httpErrors errorMode="Detailed" />
    <asp scriptErrorSentToBrowser="true" />

    <fastCgi>
      <application fullPath="C:\Users\Admin\Desktop\SDI\.venv\Scripts\python.exe"
                   arguments="C:\Users\Admin\Desktop\SDI\.venv\Lib\site-packages\wfastcgi.py">
        <environmentVariables>
          <environmentVariable name="WSGI_HANDLER" value="principal.wsgi.application" />
          <environmentVariable name="PYTHONPATH" value="C:\Users\Admin\Desktop\SDI" />
          <environmentVariable name="DJANGO_SETTINGS_MODULE" value="principal.settings" />
        </environmentVariables>
      </application>
    </fastCgi>
  </system.webServer>
</configuration>
